
{{#partial "content"}}

    <link rel="stylesheet" href="/css/users.css">

  <div class="container-fluid">
    <div class="row">
        <!-- Side menu  -->
        <div class="col-md-4"id="side">
            <ul class="navbar-nav d-flex flex-md-column align-items-center py-2 sticky-top mt-5 w-100 ">
                <li class="nav-item"><a class="nav-link" href="/dashboard">My Account</a></li>
                <li class="nav-item"><a class="nav-link" href="/users">Users</a></li>
                <li class="nav-item"><a class="nav-link" href="/bookings">Booking</a></li>
                <br>
                <br>
                <hr class="new1">
                <li class="nav-item"><a class="nav-link" href="/settings">Settings</a></li>
                <li class="nav-item"><img src="images/power.png"> <a class="nav-link" href="/logout">Logout</a></li>
            </ul>
        </div>
        <!-- main working area  -->
        <div class="col-md-8">
            <!-- Top Navigation  -->
            <div class="row-2">
                <nav class="navbar">
                    <div class="container-fluid">
                        <div class="col-md-4">
                            <img src="images/prof.png" alt="profile pic">
                        </div>
                        <div class="col-md-4">
                            <h6>Vikins Treo</h6>
                        </div>
                        <div class="col-md-4">
                            <form class="d-flex">
                                <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                            </form>
                        </div>
                        <h5 style="margin-top:10px;">Last seen: 24hours ago</h5>
                    </div>
                </nav>
            </div>
            <!-- Working area -->
            <div class="row-10">
                <br>
                <ul class="list-group">

                    {{#if users}}
                        {{#each users}}
                    <div class="row" id="one">
                        <li class="list-group-item" id="one">
                            <div class="row">
                                <div class="col-md-4">
                                    <img src="images/Ellipse 5.png" alt="profile pic">
                                </div>
                                <div class="col-md-4">
                                    <h3>Name:{{name}}</h3>
                                    <h3>Email:{{email}}</h3>
                                </div>
                                <div class="col-md-4">
                                    <img src="images/arrow.png" alt="arrow pic" id="arrow">
                                </div>
                            </div>

                        </li>
                    </div>
{{/each}}
{{/if}}

                </ul>




            </div>

        </div>


    </div>
</div>
{{/partial}}

{{> layout.hbs}}